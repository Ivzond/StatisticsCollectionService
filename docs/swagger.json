            "post": {
                "description": "Сохранить новый ордер для указанного клиента",
                "summary": "Сохранить ордер",
                "parameters": [
                    {
                        "description": "Ордер",
                        "name": "order",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.HistoryOrder"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
