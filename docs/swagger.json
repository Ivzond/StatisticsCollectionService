                        "name": "exchange_name",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Валютная пара",
                        "name": "pair",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
